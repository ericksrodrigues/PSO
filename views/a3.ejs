<html>
  <head>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawChart);

      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Iterações', 'Global', 'Local'],
          <% for(let i = 0; i < sphere[0].length ; i++){ %>
          ['<%= i+1 %>',  <%= sphere[0][i] %>,<%= sphere[1][i] %>]<% if(i != sphere[0].length - 1){ %>,<% }} %>
          
        ]);

        var options = {
          title: 'Análise 3 - Sphere',
          curveType: 'function',
          legend: { position: 'bottom' }
        };

        var chart = new google.visualization.LineChart(document.getElementById('curve_chart'));
        chart.draw(data,options);
        data = google.visualization.arrayToDataTable([
          ['Iterações', 'Global', 'Local'],
            <% for(let i = 0; i < sphere[0].length ; i++){ %>
          ['<%= i+1 %>',  <%= rotated[0][i] %>,<%= rotated[1][i] %>]<% if(i != sphere[0].length - 1){ %>,<% }} %>
            
        ]);
        options.title = "Análise 3 - Rotated";
        chart = new google.visualization.LineChart(document.getElementById('curve_chart2'));
        chart.draw(data,options);
        data = google.visualization.arrayToDataTable([
          ['Iterações', 'Global', 'Local'],
            <% for(let i = 0; i < sphere[0].length ; i++){ %>
            ['<%= i+1 %>',  <%= rosenbrock[0][i] %>,<%= rosenbrock[1][i] %>]<% if(i != sphere[0].length - 1){ %>,<% }} %>
            
        ]);
        options.title = "Análise 3 - Rosenbrock";
        var chart = new google.visualization.LineChart(document.getElementById('curve_chart3'));
        chart.draw(data, options);
      }
    </script>
  </head>
  <body>
    <div id="curve_chart" style="width: 900px; height: 500px"></div>
    <div id="curve_chart2" style="width: 900px; height: 500px"></div>
    <div id="curve_chart3" style="width: 900px; height: 500px"></div>

  </body>
</html>